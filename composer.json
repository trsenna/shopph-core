{
	"name": "trsenna/shopph-core",
	"description": "Use this project as a reference to quickly start any new core package.",
	"keywords": [],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/trsenna/shopph-core/issues",
		"source": "https://github.com/trsenna/shopph-core"
	},
	"authors": [
		{
			"name": "Thiago Senna",
			"email": "trsenna@gmail.com"
		}
	],
	"require": {
		"php": "^7.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5"
	},
	"autoload": {
		"psr-4": {
			"Shopph\\": "src/Component/",
			"Shopph\\Contract\\": "src/Contract/",
			"Shopph\\Shared\\": "src/Shared/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Shopph\\Tests\\": "tests/Component/",
			"Shopph\\Tests\\Shared\\": "tests/Shared/"
		},
		"files": [
			"tests/functions-helpers.php"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
